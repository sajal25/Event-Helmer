<p id="notice"><%= notice %></p>

<h1>Event Organizers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>About</th>
      <th>Email</th>
      <th>Website</th>
      <th>Contact</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_organizers.each do |event_organizer| %>
      <tr>
        <td><%= event_organizer.name %></td>
        <td><%= event_organizer.about %></td>
        <td><%= event_organizer.email %></td>
        <td><%= event_organizer.website %></td>
        <td><%= event_organizer.contact %></td>
        <td><%= event_organizer.address %></td>
        <td><%= link_to 'Show', event_organizer %></td>
        <td><%= link_to 'Edit', edit_event_organizer_path(event_organizer) %></td>
        <td><%= link_to 'Destroy', event_organizer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Organizer', new_event_organizer_path %>
