  <!-- LOGIN SECTION -->
  <section class="clearfix loginSection " style="margin-top: 100px">
    <div class="container">
      <div class="row">
        <div class="center-block col-md-5 col-sm-6 col-xs-12">
          <div class="panel panel-default loginPanel">
            <div class="panel-heading text-center">Members log in</div>
            <div class="panel-body">
              <form class="loginForm">
                <div class="form-group">
                  <label for="userName">Email</label>
                  <%= f.email_field :email, autofocus: true, autocomplete: "email" , class:"form-control", id:"userName"%>
                  <p class="help-block">Enter your Foundation username.</p>
                </div>
                <div class="form-group">
                  <label for="userPassword">Password *</label>
                  <%= f.password_field :password, autocomplete: "off", class:"form-control", id:"userPassword" %>
                  <p class="help-block">Enter the password that accompanies your username.</p>
                </div>
                <div class="form-group">
                  <%= f.submit "Log in", class:"btn btn-primary pull-left" %>
                  <%= render "users/shared/links" %>
                </div>
              </form>
            </div>
            <div class="panel-footer text-center">
              <p>Not a member yet? <a href="sign-up.html" class="link">Sign up</a></p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <% end %>